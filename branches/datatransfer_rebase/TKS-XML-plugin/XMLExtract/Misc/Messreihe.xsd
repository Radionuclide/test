	<xsd:complexType name="PunktType">
		<xsd:sequence>
			<xsd:element name="X" type="xsd:double"/>
			<xsd:element name="Y" type="xsd:double" minOccurs="0"/>
			<xsd:element name="Z" type="xsd:double" minOccurs="0"/>
			<xsd:element name="Wert" type="xsd:double"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LinieType">
		<xsd:sequence>
			<xsd:element name="Y" type="xsd:double" minOccurs="0"/>
			<xsd:element name="Z" type="xsd:double" minOccurs="0"/>
			<xsd:element name="Xs" type="ListOfDoubleType"/>
			<xsd:element name="WerteX" type="ListOfDoubleType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ListOfDoubleType">
		<xsd:list itemType="xsd:double"/>
	</xsd:simpleType>
	<xsd:complexType name="EinzelwertType">
		<xsd:sequence>
			<xsd:element name="Bezeichner" type="StringMax100">
				<xsd:annotation>
					<xsd:documentation>z.B. TemperaturZinkbad</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Wert" type="xsd:string"/>
			<xsd:element name="Einheit" type="EinheitEnum" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="StatistikType">
		<xsd:sequence>
			<xsd:element name="Min" type="xsd:double"/>
			<xsd:element name="Max" type="xsd:double"/>
			<xsd:element name="Avg" type="xsd:double"/>
			<xsd:element name="StdDev" type="xsd:double"/>
			<xsd:element name="Median" type="xsd:double" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DefectMapType">
		<xsd:sequence>
			<xsd:element name="MaterialHeader" type="MaterialHeaderType"/>
			<xsd:element name="DefectMapInfos">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Aggregat" type="StringMax10"/>
						<xsd:element name="Messzeitpunkt" type="xsd:dateTime"/>
						<xsd:element name="OisGeraetID" type="StringMax100"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Defects">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Defect" type="DefectType" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Beschreibt ein Rechteck auf dem Band und den dort auftretenden Fehler. Alle Positionen in mm.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DefectType">
		<xsd:sequence>
			<xsd:element name="defectId" type="xsd:unsignedInt"/>
			<xsd:element name="refHauptDefId" type="xsd:unsignedInt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Referenz auf den Hauptdefekt, wenn es sich hier um einen Teildefekt handelt.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="harmFehler" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>Fehlerschluessel FLSNeu (4st.)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lokalerFehler" type="xsd:int" minOccurs="0"/>
			<xsd:element name="Seite" type="SeiteEnum"/>
			<xsd:element name="urlBild" type="xsd:string" minOccurs="0"/>
			<xsd:element name="posMD" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>Abstand Bandanfang</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="posCD" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>Abstand Antriebsseite</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="posRCD" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>Abstand Beobachterseite</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="sizeMD" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>Breite in Bandrichtung</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="sizeCD" type="xsd:double">
				<xsd:annotation>
					<xsd:documentation>Breite senkrecht zur Bandrichtung in Bandebene. Angabe nicht redundant, da die Bandbreite an der Position u. U. nicht genau bekannt ist.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="grade" type="xsd:int" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Schweregrad</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="periodID" type="xsd:int" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Num. Bezeichnung einer periodischen Serie von Fehlern, keine Referenz</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="periodLength" type="xsd:double" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="SeiteEnum">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="OS"/>
			<xsd:enumeration value="US"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
